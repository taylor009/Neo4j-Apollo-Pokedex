    "apollo-server-express": "^2.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.3",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.14.1"
  }
