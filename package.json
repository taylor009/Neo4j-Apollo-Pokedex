{
  "name": "apollo-poke",
  "version": "0.0.1",
  "main": "server.js",
  "author": "Taylor Gagne",
  "license": "MIT",
  "proxy": "http://localhost:4001",
  "engines": {
    "node": "14.1.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "start-dev": "./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  server.js",
    "build": "babel src -d build && shx cp .env build && shx cp src/schema.graphql build && cd client && npm run build",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.3",
    "neo4j-driver": "^1.7.6",
    "neo4j-graphql-js": "^2.7.1"
  }
}
